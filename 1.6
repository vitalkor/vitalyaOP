using System;
namespace Currently
{

    class Program
    {
        static void Main(string[] args)
        {
            int a = 0;
            int b = 0;
            int c = 0;
            int k1 = 0;
            int k = 0;
            Console.WriteLine("Введите число");
            int n = int.Parse(Console.ReadLine());
            for (int i = 100; i < 1000; i++)
            {
                a = i / 100;
                b = i % 100 / 10;
                c = i % 10;
                if (a + b + c == n)
                {
                    k++;
                }

            }
            for (int i = 10; i < 100; i++)
            {
                a = i / 10;

                c = i % 10;

                if (a + c + ((a + c) * (a + c)) == i)
                {
                    k1++;
                }

            }
            Console.WriteLine(k);
            Console.WriteLine(k1);
        }
    }
}
